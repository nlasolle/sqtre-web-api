#
#################################################################


###  http://d-nb.info/standards/elementset/gnd#dateOfConferenceOrEvent

<http://d-nb.info/standards/elementset/gnd#dateOfConferenceOrEvent> rdf:type owl:DatatypeProperty .



###  http://e-hp.ahp-numerique.fr/ahpo#chapterNumber

ahpo:chapterNumber rdf:type owl:DatatypeProperty ;
               
               rdfs:subPropertyOf owl:topDataProperty ;
               
               rdfs:domain ahpo:BookChapter ;
               
               rdfs:range xsd:positiveInteger ;
               
               rdfs:comment "Le numéro du chapitre dans le livre."@fr ,
                            "The number of the chapter in the book."@en ;
               
               rdfs:label "chapterNumber"@en ,
                          "numéroDeChapitre"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#classificationNumber

ahpo:classificationNumber rdf:type owl:DatatypeProperty ;
                      
                      rdfs:subPropertyOf owl:topDataProperty ;
                      
                      rdfs:range rdfs:Literal .



###  http://e-hp.ahp-numerique.fr/ahpo#country

ahpo:country rdf:type owl:DatatypeProperty ;
         
         rdfs:subPropertyOf owl:topDataProperty ;
         
         rdfs:domain ahpo:Address ;
         
         rdfs:range xsd:string ;
         
         rdfs:comment "Name of the most global administrative unit of the address."@en ,
                      "Nom de l’unité administrative la plus globale de l’adresse."@fr ;
         
         rdfs:label "country"@en ,
                    "pays"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#documentType

ahpo:documentType rdf:type owl:DatatypeProperty ;
              
              rdfs:subPropertyOf owl:topDataProperty ;
              
              rdfs:domain ahpo:Document ;
              
              rdfs:range xsd:string ;
              
              rdfs:comment "Forme du document, tirée d’un vocabulaire contrôlé."@fr ,
                           "Kind of the document, picked in a controlled vocabulary."@en ;
              
              rdfs:label "documentType"@en ,
                         "typeDeDocument"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#earliestPossibleWritingDate

ahpo:earliestPossibleWritingDate rdf:type owl:DatatypeProperty ;
                             
                             rdfs:subPropertyOf ahpo:hypotheticalWritingDate ;
                             
                             rdfs:domain ahpo:Document ;
                             
                             rdfs:range xsd:dateTime ;
                             
                             rdfs:comment "Earliest possible date on when the document might been written. For example, for letters, it can be the writing date of the letter to which that letter replies."@en ,
                                          "La plus ancienne date à partir de laquelle le document a pu être écrit. Par exemple, pour les lettres, cela peut être la date d’écriture de la lettre à laquelle cette lettre répond."@fr ;
                             
                             rdfs:label "earliestPossibleWritingDate"@en ,
                                        "écritAuPlusTôtLe"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#editionNumber

ahpo:editionNumber rdf:type owl:DatatypeProperty ;
               
               rdfs:subPropertyOf owl:topDataProperty ;
               
               rdfs:domain ahpo:Book ;
               
               rdfs:range xsd:positiveInteger ;
               
               rdfs:comment "Numéro de version de l’édition, pour les livres réédités."@fr ,
                            "Version number of the edition for republished books."@en ;
               
               rdfs:label "editionNumber"@en ,
                          "numéroDeLÉdition"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#endPage

ahpo:endPage rdf:type owl:DatatypeProperty ;
         
         rdfs:subPropertyOf owl:topDataProperty ;
         
         rdfs:domain ahpo:Article ;
         
         rdfs:range xsd:positiveInteger ;
         
         rdfs:comment "Book or issue page where the article ends."@en ,
                      "Page du livre ou du numéro de journal où l’article s’achève."@fr ;
         
         rdfs:label "endPage"@en ,
                    "pageDeFin"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#familyName

ahpo:familyName rdf:type owl:DatatypeProperty ;
            
            rdfs:subPropertyOf owl:topDataProperty ;
            
            rdfs:domain ahpo:Person ;
            
            rdfs:range xsd:string ;
            
            rdfs:comment "Family name of the person (often the last part of the full name)."@en ,
                         "Nom de famille de la personne (souvent la dernière partie du nom complet)."@fr ;
            
            rdfs:label "familyName"@en ,
                       "nomDeFamille"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#firstName

ahpo:firstName rdf:type owl:DatatypeProperty ;
           
           rdfs:subPropertyOf owl:topDataProperty ;
           
           rdfs:domain ahpo:Person ;
           
           rdfs:range xsd:string ;
           
           rdfs:comment "Given name of the person (often the first part of the full name)."@en ,
                        "Prénom donné à la personne (souvent la première partie du nom complet)."@fr ;
           
           rdfs:label "firstName"@en ,
                      "prénom"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#fullAddress

ahpo:fullAddress rdf:type owl:DatatypeProperty ;
             
             rdfs:subPropertyOf owl:topDataProperty ;
             
             rdfs:domain ahpo:Address ;
             
             rdfs:range xsd:string ;
             
             rdfs:comment "Adresse postale complète, lisible par l’humain, qui identifie un lieu unique."@fr ,
                          "Full human-readable post address, which identifies a unique place."@en ;
             
             rdfs:label "adresseComplète"@fr ,
                        "fullAddress"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#fullTextOnlineAt

ahpo:fullTextOnlineAt rdf:type owl:DatatypeProperty ;
                  
                  rdfs:subPropertyOf owl:topDataProperty ;
                  
                  rdfs:domain ahpo:Document ;
                  
                  rdfs:range xsd:anyURI ;
                  
                  rdfs:comment "URL où une version numérique complète du document peut être trouvée."@fr ,
                               "URL where we can find a full digitised version of the document."@en ;
                  
                  rdfs:label "fullTextOnlineAt"@en ,
                             "texteCompletEnLigneSur"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#hypotheticalWritingDate

ahpo:hypotheticalWritingDate rdf:type owl:DatatypeProperty ;
                         
                         rdfs:subPropertyOf <http://purl.org/dc/terms/created> ,
                                            owl:topDataProperty ;
                         
                         rdfs:domain ahpo:Document ;
                         
                         rdfs:range xsd:dateTime ;
                         
                         rdfs:comment "Date on when the document is supposed to have been written."@en ,
                                      "Date à laquelle le document est supposé avoir été écrit."@fr ;
                         
                         rdfs:label "dateDÉcritureHypothétique"@fr ,
                                    "hypotheticalWritingDate"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#identifier

ahpo:identifier rdf:type owl:DatatypeProperty ;
            
            owl:equivalentProperty <http://purl.org/dc/terms/identifier> ;
            
            rdfs:subPropertyOf owl:topDataProperty ;
            
            rdfs:domain ahpo:Document ;
            
            rdfs:range xsd:string ;
            
            rdfs:comment "Literal which let identify the document in a given context."@en ,
                         "Littéral qui permet d’identifier le document dans un contexte donné."@fr ;
            
            rdfs:label "identifiant"@fr ,
                       "identifier"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#identifierInLocalArchives

ahpo:identifierInLocalArchives rdf:type owl:DatatypeProperty ;
                           
                           rdfs:subPropertyOf owl:topDataProperty ;
                           
                           rdfs:domain ahpo:Document ;
                           
                           rdfs:range rdfs:Literal ;
                           
                           rdfs:comment "Literal identifying the document in local archives."@en ,
                                        "Littéral identifiant le document dans les archives locales."@fr ;
                           
