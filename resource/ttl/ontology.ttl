@prefix ahpo: <http://e-hp.ahp-numerique.fr/ahpo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix o:     <http://omeka.org/s/vocabs/o#> .
@base <http://e-hp.ahp-numerique.fr/ahpo> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<http://e-hp.ahp-numerique.fr/ahpo> rdf:type owl:Ontology ;
                                    
                                    owl:versionIRI <http://e-hp.ahp-numerique.fr/ahpo/2.0> .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://www.w3.org/2000/01/rdf-schema#subPropertyOf

rdfs:subPropertyOf rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2002/07/owl#equivalentClass

owl:equivalentClass rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2002/07/owl#equivalentProperty

owl:equivalentProperty rdf:type owl:AnnotationProperty .



###  http://www.w3.org/2004/02/skos/core#altLabel

<http://www.w3.org/2004/02/skos/core#altLabel> rdf:type owl:AnnotationProperty .



dcterms:subject rdfs:label "hasSubject"@en, 
                           "aPourThème"@fr.

#################################################################
#
#    Object Properties
#
#################################################################


###  http://e-hp.ahp-numerique.fr/ahpo#archivedAt

ahpo:archivedAt rdf:type owl:ObjectProperty ;
            
            rdfs:subPropertyOf <http://purl.org/dc/terms/isPartOf> ;
            
            rdfs:domain ahpo:Document ;
            
            rdfs:range ahpo:ArchivePlace ;
            
            rdfs:comment "Lieu où le document est conservé."@fr ,
                         "Location where the document is curated."@en ;
            
            rdfs:label "archivedAt"@en ,
                       "archivéÀ"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#authoredBy

ahpo:authoredBy rdf:type owl:ObjectProperty ;
            
            rdfs:subPropertyOf <http://purl.org/dc/terms/creator> ;
            
            rdfs:domain ahpo:Document ;
            
            rdfs:range ahpo:Person ;
            
            rdfs:comment "Auteur principal du document écrit."@fr ,
                         "Main author of the written document."@en ;
            
            rdfs:label "authoredBy"@en ,
                       "écritPar"@fr .

###  http://e-hp.ahp-numerique.fr/ahpo#hasTopic

ahpo:hasTopic rdf:type owl:ObjectProperty ;
            
            rdfs:domain ahpo:Document ;
            
            rdfs:label "hasTopic"@en ,
                       "aPourSujet"@fr .


###  http://e-hp.ahp-numerique.fr/ahpo#citeApparatArgot

ahpo:citeApparatArgot rdf:type owl:ObjectProperty ;
                  
                  rdfs:subPropertyOf <http://www.co-ode.org/ontologies/ont.owl#cite> ;
                  
                  rdfs:comment "Terme d'argot polytechnique cité dans l'apparat critique." ;
                  
                  rdfs:label "citeApparatArgot" .



###  http://e-hp.ahp-numerique.fr/ahpo#citeApparatBiblio

ahpo:citeApparatBiblio rdf:type owl:ObjectProperty ;
                   
                   rdfs:subPropertyOf <http://www.co-ode.org/ontologies/ont.owl#cite> ;
                   
                   rdfs:comment "Référence bibliographique citée dans l'apparat critique." ;
                   
                   rdfs:label "citeApparatBiblio" .



###  http://e-hp.ahp-numerique.fr/ahpo#citeApparatName

ahpo:citeApparatName rdf:type owl:ObjectProperty ;
                 
                 rdfs:subPropertyOf <http://www.co-ode.org/ontologies/ont.owl#cite> ;
                 
                 rdfs:domain ahpo:Document ,
                             ahpo:Journal ,
                             <http://purl.org/ontology/bibo/Article> ,
                             <http://purl.org/ontology/bibo/Book> ,
                             <http://purl.org/ontology/bibo/Chapter> ,
                             <http://purl.org/ontology/bibo/Journal> ,
                             <http://purl.org/ontology/bibo/Letter> ,
                             <http://purl.org/ontology/bibo/Report> ,
                             <http://purl.org/ontology/bibo/Thesis> ,
                             <http://xmlns.com/foaf/0.1/Document> ;
                 
                 rdfs:comment "Personne citée dans l'apparat critique."@fr ;
                 
                 rdfs:label "citeApparatName"@en ,
                            "citeApparatNom"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#citeArgot

ahpo:citeArgot rdf:type owl:ObjectProperty ;
           
           rdfs:subPropertyOf <http://www.co-ode.org/ontologies/ont.owl#cite> ;
           
           rdfs:comment "Argot polytechnique utilisé dans le document primaire."@fr ;
           
           rdfs:label "citeArgot" .



###  http://e-hp.ahp-numerique.fr/ahpo#citeName

ahpo:citeName rdf:type owl:ObjectProperty ;
          
          rdfs:subPropertyOf <http://www.co-ode.org/ontologies/ont.owl#cite> ;
          
          rdfs:comment "Personne citée dans le document primaire." ;
          
          rdfs:label "cite"@en ,
                     "cite"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#citeOEuvre

ahpo:citeOEuvre rdf:type owl:ObjectProperty ;
            
            rdfs:subPropertyOf <http://www.co-ode.org/ontologies/ont.owl#cite> ;
            
            rdfs:comment "Oeuvre (théâtre, musique, littérature, ...) citée."@fr ;
            
            rdfs:label "citeOEuvre" .



###  http://e-hp.ahp-numerique.fr/ahpo#classificationNumber

ahpo:classificationNumber rdf:type owl:ObjectProperty ;
                      
                      rdfs:subPropertyOf <http://purl.org/dc/terms/isPartOf> ;
                      
                      rdfs:domain ahpo:Document .



###  http://e-hp.ahp-numerique.fr/ahpo#destinationAddress

ahpo:destinationAddress rdf:type owl:ObjectProperty ;
                    
                    rdfs:subPropertyOf <http://www.w3.org/ns/locn#address> ;
                    
                    rdfs:domain ahpo:Letter ;
                    
                    rdfs:range ahpo:Address ;
                    
                    rdfs:comment "Address written on the envelope. The letter has been sent to this address."@en ,
                                 "Adresse écrite sur l’enveloppe. La lettre a été envoyée à cette adresse."@fr ;
                    
                    rdfs:label "adresseDeDestination"@fr ,
                               "destinationAddress"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#editedBy

ahpo:editedBy rdf:type owl:ObjectProperty ;
          
          rdfs:subPropertyOf <http://purl.org/dc/terms/contributor> ;
          
          rdfs:domain ahpo:Document ;
          
          rdfs:range ahpo:Person ;
          
          rdfs:comment "Person who has contributed to the document writing, even in a minor way. They cannot be considered as author because they have not authored the main content."@en ,
                       "Personne ayant contribué à l’écriture du document, même d’une façon mineure. Elle ne peut pas être considérée comme auteur parce qu’elle n’est pas à l’origine du contenu central."@fr ;
          
          rdfs:label "aPourContributeur"@fr ,
                     "editedBy"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#hasAddress

ahpo:hasAddress rdf:type owl:ObjectProperty ;
            
            owl:equivalentProperty <http://www.w3.org/ns/locn#address> ;
            
            rdfs:subPropertyOf owl:topObjectProperty ;
            
            rdfs:domain ahpo:Place ;
            
            rdfs:range ahpo:Address ;
            
            rdfs:comment "Address which defines the location."@en ,
                         "Adresse qui définit le lieu."@fr ;
            
            rdfs:label "aPourAdresse"@fr ,
                       "hasAddress"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#hasDedicatedChapter

ahpo:hasDedicatedChapter rdf:type owl:ObjectProperty ;
                     
                     rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/isPrimaryTopicOf> ;
                     
                     rdfs:domain owl:Thing ;
                     
                     rdfs:range ahpo:BookChapter ;
                     
                     rdfs:comment "Book chapter which is about the resource, or only contains elements related to the resource."@en ,
                                  "Chapitre de livre qui parle de la ressource, ou qui ne contient que des éléments liés à la ressource."@fr ;
                     
                     rdfs:label "aPourChapitreDédié"@fr ,
                                "hasDedicatedChapter"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#hasReply

ahpo:hasReply rdf:type owl:ObjectProperty ;
          
          rdfs:domain ahpo:Letter ;
          
          rdfs:range ahpo:Letter ;
          
          rdfs:comment "Letter which replies to the letter."@en ,
                       "Lettre qui répond à la lettre."@fr ;
          
          rdfs:label "aPourRéponse"@fr ,
                     "hasReply"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#inBook

ahpo:inBook rdf:type owl:ObjectProperty ;
        
        rdfs:subPropertyOf <http://purl.org/dc/terms/isPartOf> ;
        
        rdfs:domain ahpo:BookChapter ;
        
        rdfs:range ahpo:Book ;
        
        rdfs:comment "Book to which the chapter belongs."@en ,
                     "Livre auquel le chapitre appartient"@fr ;
        
        rdfs:label "dansLeLivre"@fr ,
                   "inBook"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#inCollection

ahpo:inCollection rdf:type owl:ObjectProperty ;
              
              rdfs:subPropertyOf <http://purl.org/dc/terms/isPartOf> ,
                                 owl:topObjectProperty ;
              
              rdfs:domain ahpo:Book ;
              
              rdfs:range ahpo:Collection ;
              
              rdfs:comment "Collected works to which the book belongs."@en ,
                           "Collection éditoriale à laquelle le livre appartient."@fr ;
              
              rdfs:label "dansLaCollection"@fr ,
                         "inCollection"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#isAbout

ahpo:isAbout rdf:type owl:ObjectProperty ;
         
         rdfs:domain ahpo:Report ;
         
         rdfs:range ahpo:Document ;
         
         rdfs:comment "Document about which the report is."@en ,
                      "Document duquel parle le rapport."@fr ;
         
         rdfs:label "isAbout"@en ,
                    "parleDe"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#issueOf

ahpo:issueOf rdf:type owl:ObjectProperty ;
         
         rdfs:subPropertyOf <http://purl.org/dc/terms/isPartOf> ,
                            owl:topObjectProperty ;
         
         rdfs:domain ahpo:Issue ;
         
         rdfs:range ahpo:Journal ;
         
         rdfs:comment "Lie le numéro de revue à la Revue en tant qu’entité."@fr ,
                      "Links the journal issue to a journal entity."@en ;
         
         rdfs:label "estUnNuméroDe"@fr ,
                    "issueOf"@en ;
         
         <http://www.w3.org/2004/02/skos/core#altLabel> "journal"@en ,
                                                        "revue"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#liveAt

ahpo:liveAt rdf:type owl:ObjectProperty ;
        
        rdfs:subPropertyOf <http://www.w3.org/ns/locn#address> ;
        
        rdfs:domain ahpo:Person ;
        
        rdfs:range ahpo:Address ;
        
        rdfs:comment "Adresse du domicile de la personne."@fr ,
                     "Home address of the person."@en ;
        
        rdfs:label "liveAt"@en ,
                   "vitÀ"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#locatedAt

ahpo:locatedAt rdf:type owl:ObjectProperty ;
           
           rdfs:subPropertyOf <http://www.w3.org/ns/locn#address> ;
           
           rdfs:domain ahpo:Institution ;
           
           rdfs:range ahpo:Address ;
           
           rdfs:comment "Address where the institution has been settled."@en ,
                        "Adresse où est installée l’institution."@fr ;
           
           rdfs:label "locatedAt"@en ,
                      "situéÀ"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#publishedBy

ahpo:publishedBy rdf:type owl:ObjectProperty ;
             
             rdfs:subPropertyOf owl:topObjectProperty ;
             
             rdfs:domain ahpo:Book ;
             
             rdfs:range ahpo:Publisher ;
             
             rdfs:comment "Organisation qui a publié le livre."@fr ,
                          "Organisation which has published the book."@en ;
             
             rdfs:label "publishedBy"@en ,
                        "publiéPar"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#publishedIn

ahpo:publishedIn rdf:type owl:ObjectProperty ;
             
             rdfs:subPropertyOf <http://purl.org/dc/terms/isPartOf> ;
             
             rdfs:domain ahpo:Document ;
             
             rdfs:range ahpo:BookChapter ,
                        ahpo:Issue ;
             
             rdfs:comment "Journal issue or book chapter in which the document has been published."@en ,
                          "Numéro de revue ou chapitre de livre dans lequel le document a été publié."@fr ;
             
             rdfs:label "publishedIn"@en ,
                        "publiéDans"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#publishedInReference

ahpo:publishedInReference rdf:type owl:ObjectProperty ;
                      
                      rdfs:subPropertyOf <http://purl.org/dc/terms/isPartOf> ;
                      
                      rdfs:domain ahpo:Document .



###  http://e-hp.ahp-numerique.fr/ahpo#repliesTo

ahpo:repliesTo rdf:type owl:ObjectProperty ;
           
           rdfs:domain ahpo:Letter ;
           
           rdfs:range ahpo:Letter ;
           
           rdfs:comment "Letter to which the letter replies to."@en ,
                        "Lettre à laquelle répond la lettre."@fr ;
           
           rdfs:label "repliesTo"@en ,
                      "répondÀ"@fr .



###  http://e-hp.ahp-numerique.fr/ahpo#rightsHolder

ahpo:rightsHolder rdf:type owl:ObjectProperty ;
              
              rdfs:domain ahpo:Document ;
              
              rdfs:range ahpo:Institution ;
              
              rdfs:comment "Institution qui détient les droits d’auteurs sur le document."@fr ,
                           "Institution which is copyright owner of the document."@en ;
              
              rdfs:label "droitsDétenusPar"@fr ,
                         "rightsHeldBy"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#sentBy

ahpo:sentBy rdf:type owl:ObjectProperty ;
        
        rdfs:subPropertyOf <http://onto.dm2e.eu/schemas/dm2e/writer> ,
                           owl:topObjectProperty ;
        
        rdfs:domain ahpo:Letter ;
        
        rdfs:range ahpo:Person ;
        
        rdfs:comment "Auteur de la lettre ahpo: celui qui l’a écrite ou celui qui l’a signée."@fr ,
                     "Letter author: the writer or the person who sign the letter."@en ;
        
        rdfs:label "envoyéPar"@fr ,
                   "sentBy"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#sentTo

ahpo:sentTo rdf:type owl:ObjectProperty ;
        
        rdfs:subPropertyOf owl:topObjectProperty ;
        
        rdfs:domain ahpo:Letter ;
        
        rdfs:range ahpo:Person ;
        
        rdfs:comment "Addressee: the person to whom the letter is addressed."@en ,
                     "Destinataire ahpo: la personne à qui est adressée la lettre."@fr ;
        
        rdfs:label "envoyéÀ"@fr ,
                   "sentTo"@en .



###  http://e-hp.ahp-numerique.fr/ahpo#writtenAt

ahpo:writtenAt rdf:type owl:ObjectProperty ;
           
           rdfs:subPropertyOf <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#hasCreationLocation> ;
           
           rdfs:domain ahpo:Letter ;
           
           rdfs:range ahpo:Address ;
           
           rdfs:comment "Address (maybe only defining the town) where the letter has been written. It’s often written in top of the letter, next to the date in formal French letters."@en ,
                        "Adresse (définissant peut-être uniquement la ville) où la lettre a été écrite. Elle est souvent écrite en haut de la lettre, à côté de la date dans les lettres formelles françaises."@fr ;
           
           rdfs:label "writtenAt"@en ,
                      "écriteÀ"@fr .



###  http://onto.dm2e.eu/schemas/dm2e/writer

<http://onto.dm2e.eu/schemas/dm2e/writer> rdf:type owl:ObjectProperty .



###  http://purl.org/dc/terms/contributor

<http://purl.org/dc/terms/contributor> rdf:type owl:ObjectProperty .



###  http://purl.org/dc/terms/creator

<http://purl.org/dc/terms/creator> rdf:type owl:ObjectProperty .



###  http://purl.org/dc/terms/isPartOf

<http://purl.org/dc/terms/isPartOf> rdf:type owl:ObjectProperty .



###  http://www.co-ode.org/ontologies/ont.owl#cite

<http://www.co-ode.org/ontologies/ont.owl#cite> rdf:type owl:ObjectProperty .



###  http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#hasCreationLocation

<http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#hasCreationLocation> rdf:type owl:ObjectProperty .



###  http://www.w3.org/ns/locn#address

<http://www.w3.org/ns/locn#address> rdf:type owl:ObjectProperty .



###  http://xmlns.com/foaf/0.1/isPrimaryTopicOf

<http://xmlns.com/foaf/0.1/isPrimaryTopicOf> rdf:type owl:ObjectProperty .





